aws iam create-role --role-name vmimport --assume-role-policy-document  file://trust-policy.json

aws iam put-role-policy --role-name vmimport --policy-name vmimport --policy-document file://role-policy.json

aws ec2 import-image --description "Windows 2012 R2 DC No License" --license-type BYOL --disk-containers file://containers.json

aws ec2 describe-import-image-tasks --import-task-ids import-ami-08b5c4f70a4f688ae